{
  "name": "Saleor",
  "description": "High performance GraphQL-first e-commerce platform",
  "website": "https://saleor.io",
  "icon": "https://saleor.io/favicon.ico",
  "category": "ecommerce",
  "tags": ["ecommerce", "graphql", "headless", "commerce", "python", "django"],
  "inputs": {
    "admin_email": {
      "label": "Admin Email",
      "placeholder": "admin@example.com",
      "required": true,
      "type": "email",
      "description": "Administrator email address"
    },
    "admin_password": {
      "label": "Admin Password",
      "placeholder": "password",
      "required": false,
      "type": "password",
      "description": "Administrator password"
    },
    "secret_key": {
      "label": "Secret Key",
      "placeholder": "your-secret-key",
      "required": false,
      "type": "password",
      "description": "Django secret key"
    }
  },
  "services": {
    "api": {
      "exposed": true,
      "stateless": false
    },
    "dashboard": {
      "exposed": true,
      "stateless": true
    },
    "worker": {
      "internal": true
    },
    "saleor-db": {
      "internal": true,
      "managed_option": "postgres"
    },
    "saleor-redis": {
      "internal": true,
      "managed_option": "redis"
    }
  }
}
