{
  "name": "Bagisto",
  "description": "Laravel-based open source e-commerce framework",
  "website": "https://bagisto.com",
  "icon": "https://bagisto.com/favicon.ico",
  "category": "ecommerce",
  "tags": ["ecommerce", "laravel", "php", "shopping-cart", "multi-vendor"],
  "inputs": {
    "admin_email": {
      "label": "Admin Email",
      "placeholder": "admin@example.com",
      "required": true,
      "type": "email",
      "description": "Administrator email address"
    },
    "admin_password": {
      "label": "Admin Password",
      "placeholder": "password",
      "required": false,
      "type": "password",
      "description": "Administrator password"
    },
    "app_url": {
      "label": "App URL",
      "placeholder": "http://localhost:8080",
      "required": false,
      "type": "text",
      "default": "http://localhost:8080",
      "description": "Public URL for the Bagisto installation"
    }
  },
  "services": {
    "bagisto": {
      "exposed": true,
      "stateless": false
    },
    "bagisto-db": {
      "internal": true,
      "managed_option": "mysql"
    }
  }
}
